<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.jm.swcz"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="16"
        android:targetSdkVersion="16" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/icon_ship"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".login.WelcomeActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>   
        
        <activity android:name=".login.LoginActivity"></activity>
        <activity android:name=".login.RegisterActivity"></activity>
        <activity android:name=".login.RegisterConfirmActivity"></activity>
        
        <activity android:name=".tabs.MainTab"></activity>
    </application>

</manifest>
